{
  "name": "@scaleleap/amazon-marketplaces",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "A collection of Amazon Marketplace data objects.",
  "scripts": {
    "test": "jest && npm run lint",
    "build": "pack build",
    "build:docs": "typedoc",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "postpublish": "node update-package-version.js"
  },
  "keywords": [
    "amazon",
    "marketplace",
    "amazon-marketplace",
    "mws",
    "amazon-advertising",
    "amazon-advertising-api",
    "amazon-mws"
  ],
  "author": {
    "name": "Roman Filippov",
    "email": "roman@scaleleap.com",
    "url": "https://www.scaleleap.com/"
  },
  "license": "MIT",
  "devDependencies": {
    "@pika/pack": "0.4.0",
    "@pika/plugin-build-node": "0.5.1",
    "@pika/plugin-build-umd": "0.5.1",
    "@pika/plugin-build-web": "0.5.1",
    "@pika/plugin-ts-standard-pkg": "0.5.1",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/commit-analyzer": "6.2.0",
    "@semantic-release/git": "7.0.16",
    "@semantic-release/npm": "5.1.13",
    "@semantic-release/release-notes-generator": "7.2.1",
    "@types/jest": "24.0.16",
    "@types/node": "12.6.9",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "semantic-release": "15.13.19",
    "ts-jest": "24.0.2",
    "typedoc": "0.15.0",
    "typescript": "3.5.3"
  },
  "dependencies": {},
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "exclude": [
            "tests/**/*"
          ]
        }
      ]
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "pkg"
        }
      ],
      "@semantic-release/git",
      {
        "assets": [
          "package-lock.json",
          "package.json"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  }
}
